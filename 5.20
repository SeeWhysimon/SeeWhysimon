#include <iostream>
#include <vector>

int main()
{
	std::string preword = "", word = "";
	int cnt = 0;

	while (std::cin >> word) {
		if (word == preword) {
			++cnt;
			break;
		}
		else preword = word;
	}
	if (cnt == 0) std::cout << "no word was repeated" << std::endl;
	else std::cout << preword << " repeated twice" << std::endl;
	return 0;
}
